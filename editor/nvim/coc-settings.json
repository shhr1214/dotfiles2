{
  "coc.source.file.enable": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnSaveFiletypes": [
    "c",
    "cs",
    "cpp",
    "css",
    "go",
    "haskell",
    "html",
    "javascript",
    "json",
    "kotlin",
    "python",
    "ruby",
    "rust",
    "sh",
    "typescript"
  ],
  "deno.enable": false,
  "deno.lint": false,
  "deno.config": "/usr/local/bin/deno",
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "yarn",
  "languageserver": {}
}
